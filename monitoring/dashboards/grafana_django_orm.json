{
  "dashboard": {
    "id": null,
    "uid": "django-orm-monitoring",
    "title": "Django ORM Performance Monitoring",
    "tags": ["django", "orm", "performance"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "type": "graph",
        "title": "Response Time Percentiles",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "django_response_time_seconds{quantile=\"0.5\"}",
            "legendFormat": "p50"
          },
          {
            "expr": "django_response_time_seconds{quantile=\"0.95\"}",
            "legendFormat": "p95"
          },
          {
            "expr": "django_response_time_seconds{quantile=\"0.99\"}",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Response Time"
          }
        ]
      },
      {
        "id": 2,
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "type": "graph",
        "title": "Database Query Performance",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "django_query_time_seconds{quantile=\"0.5\"}",
            "legendFormat": "p50"
          },
          {
            "expr": "django_query_time_seconds{quantile=\"0.95\"}",
            "legendFormat": "p95"
          },
          {
            "expr": "django_query_time_seconds{quantile=\"0.99\"}",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Query Time"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "name": "Slow Database Queries",
          "message": "Database queries p95 exceeds 100ms"
        }
      },
      {
        "id": 3,
        "gridPos": {"x": 0, "y": 8, "w": 8, "h": 6},
        "type": "stat",
        "title": "Cache Hit Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(django_cache_hits_total[5m]) / (rate(django_cache_hits_total[5m]) + rate(django_cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 70}
            ]
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"x": 8, "y": 8, "w": 8, "h": 6},
        "type": "stat",
        "title": "Error Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(django_errors_total[5m]) / rate(django_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"x": 16, "y": 8, "w": 8, "h": 6},
        "type": "gauge",
        "title": "System Health Score",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "(1 - (rate(django_errors_total[5m]) / rate(django_requests_total[5m]))) * (rate(django_cache_hits_total[5m]) / (rate(django_cache_hits_total[5m]) + rate(django_cache_misses_total[5m]))) * 100",
            "legendFormat": "Health Score"
          }
        ],
        "options": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "green", "value": 80}
            ]
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"x": 0, "y": 14, "w": 24, "h": 8},
        "type": "table",
        "title": "Slow Queries",
        "datasource": "Django API",
        "targets": [
          {
            "url": "/monitoring/performance/queries/",
            "format": "json",
            "jsonPath": "$.slow_queries"
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "width": 150
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "time"},
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                },
                {
                  "id": "custom.width",
                  "value": 100
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}