{
    "config_version": "1.4",
    "storage_dir": "/root/mindsdb",
    "log": {
        "level": {
            "console": "INFO",
            "file": "INFO",
            "db": "WARNING"
        }
    },
    "debug": false,
    "integrations": {
        "default_postgres": {
            "enabled": true,
            "host": "postgres",
            "port": 5432,
            "user": "youtility5_user",
            "password": "password",
            "database": "youtility5_db",
            "publish": true
        },
        "redis": {
            "enabled": true,
            "host": "redis",
            "port": 6379,
            "db": 0
        },
        "web": {
            "enabled": true
        },
        "email": {
            "enabled": false
        },
        "slack": {
            "enabled": false
        }
    },
    "api": {
        "http": {
            "host": "0.0.0.0",
            "port": 47334
        },
        "mysql": {
            "host": "0.0.0.0",
            "port": 47335,
            "user": "mindsdb",
            "password": "mindsdb",
            "database": "mindsdb"
        },
        "mongodb": {
            "host": "0.0.0.0",
            "port": 47336,
            "database": "mindsdb"
        }
    },
    "cache": {
        "type": "redis",
        "params": {
            "host": "redis",
            "port": 6379,
            "db": 1
        }
    },
    "ml_task_queue": {
        "type": "redis",
        "params": {
            "host": "redis",
            "port": 6379,
            "db": 2
        }
    },
    "jobs": {
        "disable": false
    },
    "permanent_storage": {
        "location": "local"
    },
    "cloud": {
        "api": "https://cloud.mindsdb.com",
        "enabled": false
    },
    "ml_engines": {
        "lightwood": {
            "enabled": true
        },
        "huggingface": {
            "enabled": true
        },
        "openai": {
            "enabled": true
        },
        "statsforecast": {
            "enabled": true
        },
        "neuralforecast": {
            "enabled": true
        }
    },
    "auth": {
        "http_auth_enabled": false,
        "username": "mindsdb",
        "password": "mindsdb"
    }
}